[core]
  excludesfile = ~/.gitignore
	autocrlf = input

[push]
  default = simple

[pull]
  ff = only

[merge]
  conflictstyle = diff3

[mergetool]
  prompt = false

[color]
  ui = true

[credential]
	helper = osxkeychain

[init]
	defaultBranch = main
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
